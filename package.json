{
  "name": "redux-in-worker",
  "description": "Entire Redux in Web Worker",
  "version": "0.1.1",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/redux-in-worker.git"
  },
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "compile": "babel src -d dist",
